    "joi": "^17.11.0",
    "redis": "^4.6.10",
    "@supabase/supabase-js": "^2.38.4",
    "googleapis": "^129.0.0",
    "node-cron": "^3.0.3",
    "express-rate-limit": "^7.1.5"
  },
  "devDependencies": {
